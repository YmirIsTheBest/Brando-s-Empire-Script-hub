local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Brando Empire", "Ocean")

--Project Slayer Tab
local Main = Window:NewTab("Project Slayer")
local MainSection = Main:NewSection("Scripts")


MainSection:NewButton("Brando Hub", "This Script Requires Synapse X To Run", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LeadMarker/Scripts/main/ProjectSlayer.lua"))()
end)

MainSection:NewToggle("ToggleText", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)



MainSection:NewButton("keybrew Hub ", "Keyless On Top", function()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/borntodiekuv/KeybrewHub/main/Main"))()
        end)


        MainSection:NewButton("Lazy Hub", "Script #3", function()
            repeat wait() until game:IsLoaded()

loadstring(game:HttpGet("https://raw.githubusercontent.com/LioK251/RbScripts/main/lazyhub.lua"))()
        end)



        MainSection:NewButton("Bruhnhe Hub ", "bruh Hub", function()
            if game.PlaceId == 6152116144 then
                pcall(function()loadstring(game:HttpGet("https://raw.githubusercontent.com/connorobrien11/wbat.file/main/C4U6KFq6T6BatBvpFyTBzwiDCMhTPh%25%26type%3Dza"))()end)
            else 
                warn('incorrect game') 
                game:Shutdown()
            end
                   end)

                   --Grand Piece Online
                   local Main = Window:NewTab("Grand Piece Online")
local MainSection = Main:NewSection("Scripts")


MainSection:NewButton("Brando Hub", "This Script Requires Synapse X To Run", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LeadMarker/Scripts/main/ProjectSlayer.lua"))()
end)
